#include <iostream>
#include <math.h>
using namespace std;
 
int main ()
{
  float x0=0, x1=1, y0=0, y1=1, R=1, L=0;
 
  // Задаем координаты окружности: x0,y0 - центр, R -радиус
  cout << "x0= ";
  cin >> x0;
  cout << "y0= ";
  cin >> y0;
  cout << "R= ";
  cin >> R;
 
  // Задаем координаты точки
  cout << "x1= ";
  cin >> x1;
  cout << "y1= ";
  cin >> y1;
 
  // Находим длинну отрезка между центром окружности
  // и данной точкой
  L= sqrt( (x1-x0)*(x1-x0) + (y1-y0)*(y1-y0));
  if (L<=R)
  cout << " IN circle" << "\n";
  else
  cout << " OUT circle" << "\n";
  return 0;
}