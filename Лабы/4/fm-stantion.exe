#include <iostream>
#include <vector>
#include <string>
#include <sstream>

int main() {
   std::string s = R"x(NEWS
DAYS
anecdotes
week
Triangle (exiiit)
)x"; 
    std::istringstream iss(s); 
    std::vector<std::string> lines; 
    std::string line; 
    while(getline(iss,line)) { 
        lines.push_back(line); 
    }
    for(const auto& line : lines) {
        std::cout << line << '\n';
    }
}