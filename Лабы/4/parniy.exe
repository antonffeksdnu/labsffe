#include<iostream>
using namespace std;
 
bool check(int d)
{
    return d % 2 == 0;
}
 
int main() 
{
    int count = 0;
    int arr[30];
 
    for (int i = 0; i < 30; i++) 
    {
        cout << "[" << i + 1 << "]" << ": ";
        cin >> arr[i];
        if (check(arr[i]))
            count++;
    }
 
    cout << count << endl;
 
    count = 0;
    int arr2[6][5]; //всего 30
 
    for (int i = 0; i < 6; i++) 
    {
        for (int j = 0; j < 5; j++)
        {
            cout << "[" << i + 1 << ", " << j + 1 << "]" << ": ";
            cin >> arr2[i][j];
            if (check(arr2[i][j]))
                count++;
        }
    }
    cout << count << endl;
 
    return 0;
}